{
  "name": "react-comments",
  "description": "React commenting system",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "mit",
  "scripts": {
    "start": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "client": "npm start --prefix ../client",
    "server:client": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.19",
    "mongoose-paginate-v2": "^1.3.16",
    "unique-string": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongoose-paginate-v2": "^1.3.11",
    "@types/node": "^16.4.6",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
